{
  "kind": "collectionType",
  "collectionName": "achievements",
  "info": {
    "singularName": "achievement",
    "pluralName": "achievements",
    "displayName": "ThanhTich"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "year": {
      "type": "integer"
    },
    "level": {
      "type": "enumeration",
      "enum": ["Quận", "Tỉnh", "Quốc gia", "Quốc tế"]
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images"]
    },
    "seo": {
      "type": "component",
      "component": "shared.seo"
    }
  }
}
